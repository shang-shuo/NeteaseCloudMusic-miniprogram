.search {
  width: 100%;
  margin: 10rpx 0;
}
.cell{
  width: 100%;
}
.cell-icon{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-end;
}
.all-play{
  height: 35px;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.all-play-icon{
  font-size: 35px;
  color: red;
  margin:0 14rpx 0 20rpx;
}
.all-play-text{
  font-weight: 200;
}
.van__title{
  flex: 4 !important
}
.van__value{
  flex: 1 !important
}
.tag{
  display: flex;
  width: 100%;
  justify-content: flex-start;
}
.tag-root{
  flex: 5;
  margin: 0 10rpx;
}
.tag-custom{
  margin: 10rpx 10rpx;
}
.tag-icon{
  flex: 1
}
.tag-icon-in{
  margin-top: 10rpx;
}